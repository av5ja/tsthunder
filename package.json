{
  "name": "@av5ja/tsthunder",
  "version": "0.0.2",
  "description": "ThunderAPIでの通信の型を定義したライブラリです",
  "author": "tkgstrator",
  "license": "MIT",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist"],
  "repository": {
    "type": "git",
    "url": "https://github.com/tsshogi/kanna.git"
  },
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "scripts": {
    "build": "bun build src/index.ts --sourcemap=external --target bun --minify --outdir dist",
    "prebuild": "rimraf dist",
    "postbuild": "bunx tsc && tsc-alias",
    "lint": "bunx @biomejs/biome lint --write src",
    "format": "bunx @biomejs/biome format --write src",
    "deploy": "wrangler deploy --minify src/index.ts",
    "prepare": "husky"
  },
  "dependencies": {
    "crypto-js": "^4.2.0",
    "dayjs": "^1.11.13",
    "hono": "^4.6.3",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.2",
    "@commitlint/cli": "^19.5.0",
    "@commitlint/config-conventional": "^19.5.0",
    "@commitlint/cz-commitlint": "^19.5.0",
    "@types/bun": "^1.1.10",
    "@types/crypto-js": "^4.2.2",
    "@types/lodash": "^4.17.9",
    "@types/node": "^22.7.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "rimraf": "^6.0.1",
    "tsc-alias": "^1.8.10",
    "zod": "^3.23.8"
  }
}
